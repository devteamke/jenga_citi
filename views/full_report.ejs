                  <a href="#">County: </a><%=report.location.county%><br>
                  <a href="#">Constituency: </a><%=report.location.constituency%><br>
                   <%if(report.isUrgent==true){var notify="Yes"}else {var notify="No"}%>
                  <a href="#">Notify when solved: </a><%=notify%><br>
                  <a href="#">Date / Time of submission: </a><%=moment(date).format('LLLL')%><br>
               </h3>
              </div>
  </li>
    <!-- END timeline item -->
   <%if(report.images.length>0){%> 
   <li>
