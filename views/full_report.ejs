          <div class="timeline-item">
          
  
            
  
              <div class="timeline-body">
                <div class="box box-success">
                  <div class="box-header with-border">
                    <h3 class="box-title">Actions</h3>
                  </div>
                  <div class="box-body">
                    <form action="/reports_s/<%=report._id%>/view" method="POST">
                      <div class="form-group">
                        <label for="Status" class="col-sm-2 control-label">Change status to:</label>
      
                        <div class="col-sm-10">
                        <select name="status"class="form-control select2 select2-hidden-accessible" style="width: 100%;" tabindex="-1" aria-hidden="true" required>
                            
                            <option disabled="disabled">Reported</option>
                         
                            <option value='reviewed'>Reviewed</option>
                            <option value='escalated'>Escalated</option>
                            <option value='solved'>Solved</option>
                            
                         </select>
                        </div>
                      </div> 
                      <br>
                      <div class="form-group">
                        <div class="col-sm-2 ">
                        <label for="Status" class="control-label">Reason:</label>
                        </div>
                        <div class="col-sm-10">
                        <textarea required name="reason" class="form-control" rows="3" placeholder="Enter ..." data-gramm="true" data-txt_gramm_id="73cd6558-0d6d-2af5-5538-543586141d75" data-gramm_id="73cd6558-0d6d-2af5-5538-543586141d75" spellcheck="false" data-gramm_editor="true" style="z-index: auto; line-height: 20px; font-size: 14px; transition: none 0s ease 0s; background: transparent !important;"></textarea>
                        </div>
                      </div> 
                      <div class="form-group">
                        
                        <label for="Status" class="col-sm-2 control-label"></label>
                        <div class="col-sm-10">
                          <input type='submit' value='Submit changes' class='btn btn-success pull-right' style="border-radius:0;padding:1em">
                        </div>
                      </div>
                    <form>
                  </div>
              <!-- /.box-body -->
                </div>
              </div>
  
            
          </div>
    </li>
  <% }%>
  <%if(report.reviewed.length>0){%>
    <li>
        <!-- timeline icon -->
        <i class="fa fa-comments bg-black"></i>
        <div class="timeline-item">
        <div class="box">
            <div class="box-header">
              <h3 class="box-title">Reviews</h3>

              <div class="box-tools">
                <div class="input-group input-group-sm" style="width: 150px;">
                  <input type="text" name="table_search" class="form-control pull-right" placeholder="Search">

                  <div class="input-group-btn">
                    <button type="submit" class="btn btn-default"><i class="fa fa-search"></i></button>
                  </div>
                </div>
              </div>
            </div>
            <!-- /.box-header -->
            <div class="box-body table-responsive no-padding">
              <table class="table table-hover">
                <tbody><tr>
                  <th>By</th>
                  <th>Action</th>
                  <th>Reason</th>
                  <th>At</th>
                  
                </tr>
                <%report.reviewed.forEach((review)=>{%>
                  <tr>
                    <td><%=capitalize(review.by)%></td>
                    <td>Changed status to <span class="label label-success"><%=review.action%></span></td>
                    <td><%=review.reason%></td>
                    <td><%=review.at%></td>
                    
                  </tr>
                <% })%>
              </tbody></table>
            </div>
            <!-- /.box-body -->
          </div>

          
        </div>
  </li>
  <% }else{%>
     <li>
        <!-- timeline icon -->
        <i class="fa fa-comments bg-black"></i>
        <div class="timeline-item">
        

            <h3 style="text-align:center;padding:1em;"><span >No reviews yet</span></h3>
          
        </div>
  </li>
  <% }%>
</ul>
