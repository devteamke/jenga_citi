          <%if(report.status=="solved"){var label="success"}else if(report.status=="escalated"){var label="danger"}else if(report.status=="reported"){var label="info"}%>
          <%if(report.isUrgent==true){var urgent="Yes"}else {var urgent="No"}%>
           <span class="time label label-<%=label%>" style="border-radius:0;height: 38px; ">Status: <%=capitalize(report.status)%></span>
        

            <h3 class="timeline-header"><a href="#">Type:  </a> <%=capitalize(report.type)%>  &nbsp&nbsp&nbsp&nbsp&nbsp<a href="#">Votes: </a>   <i class="fa fa-arrow-circle-up"style="color:green"></i> <%=report.upvotes.number%>&nbsp&nbsp&nbsp<a href="#" ><i class="fa fa-arrow-circle-down " style="color:red"> </i></a> <%=report.downvotes.number%>&nbsp&nbsp <a href="#" >Urgent</a>: <%=urgent%></h3>

            <div class="timeline-body">
              <%=capitalize(report.comment)%>
            </div>

           
        </div>
    </li>
    <!-- END timeline item -->
    <li>
              <i class="fa fa-user bg-aqua"></i>

              <div class="timeline-item">
               

