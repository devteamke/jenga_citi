<!--initialize table-->
<script>
$(document).ready( function () {
    // $('#reports').DataTable();
    if(screen.width<600){
    var table =  $('#reports').DataTable({"scrollX": true, dom: 'Bfrtip',
        buttons: [
            'copy', 'csv', 'excel', 'pdf', 'print'
        ]});
    }else{
    var table =  $('#reports').DataTable({
        dom: 'Bfrtip',
        buttons: [
            'copy', 'csv', 'excel', 'pdf', 'print'
        ]
    } );
    }
    $('.type').change(function(){
      table.column(1).search(this.value).draw();
    })
    $('input').iCheck({
    checkboxClass: 'icheckbox_square-blue',
    radioClass: 'iradio_square-blue',
    increaseArea: '20%' // optional
    });
    $('.type').on('ifChecked', function(event){
     
      table.column(1).search(this.value).draw();
      // $('.type2').iCheck('uncheck');
      // $('.type3').iCheck('uncheck');
    });
    $('.type2').on('ifChecked', function(event){
    
      table.column(4).search(this.value).draw();
      // $('.type').iCheck('uncheck');
      // $('.type3').iCheck('uncheck');
    }); 
    $('.type3').on('ifChecked', function(event){
    
      table.column(3).search(this.value).draw();
      // $('.type2').iCheck('uncheck');
      // $('.type1').iCheck('uncheck');
    });
    $('#resetB').click(function(){
        table.column(3).search('').draw(); 
        table.column(4).search('').draw();
        table.column(1).search('').draw();
          $('.type3').iCheck('uncheck');
          $('.type2').iCheck('uncheck');
          $('.type').iCheck('uncheck');
    });
} );
</script>
</body>
</html>
